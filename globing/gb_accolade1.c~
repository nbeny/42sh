#include "globing.h"

t_new	*make_comma(t_new *new, char *str)
{
	char	**split;
	t_new	*s;
	t_new	*izi;
	int		i;

	izi = NULL;
	s = new;
	split = ft_strsplit(str);
	if (s != NULL)
	{
		izi = add_joinaccolade(new, split[0]);
		while (s != NULL)
		{
			i = 0;
			while (split && split[i])
			{
				
				i++;
			}
			s = s->next;
		}
	}
	else
	{
		
	}
	return (izi);
}
